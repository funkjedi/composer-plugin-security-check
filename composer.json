{
  "name": "funkjedi/composer-plugin-security-check",
  "description": "Plugin for Composer to check if your application uses dependencies with known security vulnerabilities.",
  "license": "MIT",
  "type": "composer-plugin",
  "require": {
    "composer-plugin-api": "^1.0 || ^2.0",
    "sensiolabs/security-checker": "^5.0 || ^6.0"
  },
  "require-dev": {
    "composer/composer": "^2.0",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "ComposerSecurityCheck\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "ComposerSecurityCheck\\SecurityCheckPlugin"
  }
}
